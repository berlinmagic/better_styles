      <tr>
        <td>CSS: </td>
        <td class="col-12">
          <code>@import "better/navigations/nav_list";</code>
        </td>
      </tr>
      <tr>
        <td>JS: </td>
        <td class="col-12">
          <code>//= betterjs/navigations/nav_list</code>
